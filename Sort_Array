Sort the Array

public class SampleJavaP 
{
	public static void main(String[] args) {
		int k[] = {1,3,2,4,5,8,6,54,232,23,12,32,433};
		
		int i;
		
		for(int m=0; m < k.length -1 ; m++)
		{
		    if(k[m] > k[m+1])
		    {
		      i       =  k[m];
		      k[m]    =  k[m+1];
		      k[m+1]  =  i;
		    }
		}
		for(int h :  k)
		{
			System.out.println(h);
		}
	}
}
